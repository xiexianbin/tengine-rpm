/var/log/tengine/*.log {
        daily
        missingok
        rotate 52
        compress
        delaycompress
        notifempty
        create 640 tengine adm
        sharedscripts
        postrotate
                if [ -f /var/run/tengine.pid ]; then
                        kill -USR1 `cat /var/run/tengine.pid`
                fi
        endscript
}